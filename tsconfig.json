{
    "compilerOptions": {
        "strict": true,
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "esModuleInterop": true,
        "pretty": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "build"
    },
    "include": [
        "**/*.ts"
    ]
}



